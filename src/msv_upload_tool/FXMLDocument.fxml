<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="308.0" prefWidth="387.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="msv_upload_tool.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="30.0" layoutY="216.0" onAction="#handleButtonAction" prefHeight="27.0" prefWidth="327.0" text="Start Upload" />
        <Label fx:id="label" layoutX="55.0" layoutY="146.0" minHeight="16" minWidth="69" prefHeight="17.0" prefWidth="278.0" textOverrun="LEADING_WORD_ELLIPSIS" />
      <ProgressBar fx:id="uploadProgress" layoutX="30.0" layoutY="258.0" prefHeight="20.0" prefWidth="278.0"  />
      <Button fx:id="button2" layoutX="30.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleFileOpen" prefHeight="27.0" prefWidth="327.0" text="Choose File" />
      <Separator layoutX="94.0" layoutY="169.0" prefWidth="200.0" />
      <Label fx:id="label2" layoutX="55.0" layoutY="178.0" prefHeight="17.0" prefWidth="278.0" />
      <Label fx:id="notification" layoutX="172.0" layoutY="41.0">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Button fx:id="cancelUpload" onAction="#cancelUpload" layoutX="320.0" layoutY="255.0" mnemonicParsing="false" text="X" />
    </children>
</AnchorPane>
